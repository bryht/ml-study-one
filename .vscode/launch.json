    "version": "0.2.0",
    "configurations": [
        {
            "name": "data-input-api",
            "cwd": "${workspaceFolder}/src/data-input-api",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "env": {
                "MONGODB_URL": "localhost:27017"
            },
            "jinja": true

        },
        {
            "name": "data-output-api",
            "cwd": "${workspaceFolder}/src/data-output-api",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload"
            ],
            "env": {
                "MONGODB_URL": "localhost:27017"
            },
            "jinja": true

        },
        {
            "name": "data-robot",
            "cwd": "${workspaceFolder}/src/data-robot",
            "type": "python",
            "module": "main",
            "request": "launch",
            "env": {
                "DATA_INPUT_URL": "http://localhost:6001"
            }
        }
    ]
}